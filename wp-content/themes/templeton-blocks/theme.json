{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#455be2",
					"name": "Primary Blue",
					"slug": "primary-blue"
				},
				{
					"color": "#832e92",
					"name": "Primary Purple",
					"slug": "primary-purple"
				},
				{
					"color": "#018e75",
					"name": "Primary Teal",
					"slug": "primary-teal"
				},
				{
					"color": "#262626",
					"name": "Dark Gray",
					"slug": "dark-gray"
				},
				{
					"color": "#f6b6e9",
					"name": "Light Pink",
					"slug": "light-pink"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				}
			]
		},
		"layout": {
			"contentSize": "1000px",
			"wideSize": "1400px"
		},
		"spacing": {
			"spacingScale": {
				"theme": {
					"steps": 0
				}
			},
			"spacingSizes": [
				{
					"name": "Small",
					"size": "1rem",
					"slug": "30"
				},
				{
					"name": "Medium",
					"size": "2rem",
					"slug": "40"
				},
				{
					"name": "Large",
					"size": "3rem",
					"slug": "50"
				},
				{
					"name": "Extra Large",
					"size": "4rem",
					"slug": "60"
				},
				{
					"name": "2X Large",
					"size": "5rem",
					"slug": "70"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": true,
			"fontFamilies": [
				{
					"fontFamily": "Poppins, sans-serif",
					"name": "Poppins",
					"slug": "poppins"
				},
				{
					"fontFamily": "Open Sans, sans-serif",
					"name": "Open Sans",
					"slug": "open-sans"
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "0.875rem"
					},
					"name": "Large",
					"size": "1.125rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "1.5rem",
						"min": "1.25rem"
					},
					"name": "Extra Large",
					"size": "1.5rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.5rem"
					},
					"name": "2X Large",
					"size": "2rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"max": "2.5rem",
						"min": "2rem"
					},
					"name": "3X Large",
					"size": "2.5rem",
					"slug": "xxx-large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2.5rem"
					},
					"name": "4X Large",
					"size": "3rem",
					"slug": "xxxx-large"
				},
				{
					"fluid": {
						"max": "6rem",
						"min": "1.5rem"
					},
					"name": "Hero Title",
					"size": "6rem",
					"slug": "hero-title"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "1.75rem"
					},
					"name": "Heading Large",
					"size": "3rem",
					"slug": "heading-large"
				}
			],
			"fontWeight": true,
			"letterSpacing": true,
			"textDecoration": true,
			"textTransform": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/heading": {
				"spacing": {
					"margin": {
						"bottom": "1.5rem"
					}
				},
				"typography": {
					"fontWeight": "700"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxxx-large)",
					"lineHeight": "1.1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "1.3"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "1.4"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.5"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "1.5"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontWeight": "600",
					"lineHeight": "1.2"
				}
			},
			"link": {
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	]
}